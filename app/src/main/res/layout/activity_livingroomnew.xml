<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#282424"
    tools:context=".livingroomnew">

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="745dp"
        android:layout_height="373dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.32"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/control" />


    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Living Room"
        android:textColor="@color/black"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.531"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.067" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView3"
        android:layout_width="155dp"
        android:layout_height="162dp"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="180dp"
        android:backgroundTint="#393535"
        app:cardCornerRadius="20dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.021"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.65">

        <TextView
            android:id="@+id/textView17"
            android:layout_width="75dp"
            android:layout_height="43dp"
            android:text="38%"
            android:textColor="@color/white"
            android:textSize="40dp" />

        <TextView
            android:id="@+id/textView30"
            android:layout_width="136dp"
            android:layout_height="39dp"
            android:layout_gravity="center_horizontal"
            android:layout_margin="50dp"
            android:layout_marginTop="22dp"
            android:text="    Humidifi Air"
            android:textColor="@color/white"
            android:textSize="17dp" />

        <Switch
            android:id="@+id/switch2"
            android:layout_width="136dp"
            android:layout_height="39dp"
            android:layout_gravity="center_horizontal"
            android:layout_margin="106dp"
            android:layout_marginTop="22dp"
            android:text="Switch"
            android:textColor="@color/white"
            android:textSize="19dp"
            android:thumbTint="#FFB267" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView4"
        android:layout_width="155dp"
        android:layout_height="162dp"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="180dp"
        android:backgroundTint="#393535"
        app:cardCornerRadius="20dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.931"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.65">

        <Switch
            android:id="@+id/switch1"
            android:layout_width="136dp"
            android:layout_height="39dp"
            android:layout_gravity="center_horizontal"
            android:layout_margin="106dp"
            android:layout_marginTop="22dp"
            android:text="Switch"
            android:textColor="@color/white"
            android:textSize="19dp"
            android:thumbTint="#FFB267" />

        <TextView
            android:id="@+id/textView29"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="69%"
            android:textColor="@color/white"
            android:textSize="40dp" />

        <TextView
            android:id="@+id/textView31"

            android:layout_width="136dp"


            android:layout_height="39dp"
            android:layout_gravity="center_horizontal"
            android:layout_margin="50dp"
            android:layout_marginTop="22dp"
            android:text="       Purifie Air"
            android:textColor="@color/white"
            android:textSize="17dp" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView5"
        android:layout_width="360dp"
        android:layout_height="150dp"
        android:layout_margin="150dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="180dp"
        android:backgroundTint="#393535"
        app:cardCornerRadius="20dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <SeekBar
            android:id="@+id/seekBar3"
            android:layout_width="277dp"
            android:layout_height="51dp"
            android:layout_margin="69dp"
            android:layout_marginTop="50dp"
            android:max="100"

            android:progress="50"

            android:thumbTint="#FFB267" />

        <SeekBar
            android:id="@+id/seekBar"
            android:layout_width="277dp"
            android:layout_height="51dp"
            android:layout_margin="19dp"
            android:layout_marginTop="12dp"
            android:max="100"
            android:progress="50"
            android:thumbTint="#FFB267" />

        <TextView
            android:id="@+id/textView32"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="          Main light"
            android:textColor="@color/white" />

        <TextView
            android:id="@+id/textView33"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="60dp"

            android:text="          floor lamp"

            android:textColor="@color/white" />
    </androidx.cardview.widget.CardView>

    <Button
        android:id="@+id/button23"
        android:layout_width="307dp"
        android:layout_height="52dp"
        android:backgroundTint="#FFB267"
        android:text="Control Switch"
        android:textSize="17dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.586"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.867" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.146"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.938"
        app:srcCompat="@drawable/homenew1" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.397"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.938"
        app:srcCompat="@drawable/notification2" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.685"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.938"
        app:srcCompat="@drawable/search2" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.936"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.938"
        app:srcCompat="@drawable/account2" />

</androidx.constraintlayout.widget.ConstraintLayout>
